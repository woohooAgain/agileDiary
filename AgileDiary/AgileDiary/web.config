<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <location path="." inheritInChildApplications="false">
    <system.webServer>
      <handlers>
        <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
      </handlers>
      <aspNetCore processPath="dotnet" stdoutLogEnabled="true" arguments="-argFile IISExeLauncherArgs.txt">
        <environmentVariables>
          <environmentVariable name="ASPNERCORE_ENVIRONMENT" value="Development"/>
        </environmentVariables>
      </aspNetCore>
    </system.webServer>
  </location>
</configuration>